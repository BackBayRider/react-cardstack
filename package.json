    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^2.9.0",
    "eslint-config-keystone": "^2.2.0",
    "eslint-plugin-react": "^5.0.1",
    "react-dom": "^15.0.2",
